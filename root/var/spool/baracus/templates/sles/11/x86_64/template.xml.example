<?xml version="1.0"?>
<!DOCTYPE profile>
<profile xmlns="http://www.suse.com/1.0/yast2ns" xmlns:config="http://www.suse.com/1.0/configns">
  <add-on>
    <add_on_products config:type="list">
__ADDON__
    </add_on_products>
  </add-on>
  <bootloader>
    <device_map config:type="list">
      <device_map_entry>
        <firmware>hd0</firmware>
        <linux>__ROOTDISK__</linux>
      </device_map_entry>
    </device_map>
    <global>
      <activate>true</activate>
      <boot_root>true</boot_root>
      <default>SUSE Linux Enterprise Server 11 - 2.6.27.19-5</default>
      <generic_mbr>true</generic_mbr>
      <gfxmenu>/boot/message</gfxmenu>
      <lines_cache_id>2</lines_cache_id>
      <timeout config:type="integer">8</timeout>
    </global>
    <initrd_modules config:type="list">
      <initrd_module>
        <module>processor</module>
      </initrd_module>
      <initrd_module>
        <module>thermal</module>
      </initrd_module>
      <initrd_module>
        <module>ata_piix</module>
      </initrd_module>
      <initrd_module>
        <module>ata_generic</module>
      </initrd_module>
      <initrd_module>
        <module>piix</module>
      </initrd_module>
      <initrd_module>
        <module>ide_pci_generic</module>
      </initrd_module>
      <initrd_module>
        <module>fan</module>
      </initrd_module>
      <initrd_module>
        <module>jbd</module>
      </initrd_module>
      <initrd_module>
        <module>ext3</module>
      </initrd_module>
      <initrd_module>
        <module>edd</module>
      </initrd_module>
    </initrd_modules>
    <loader_type>grub</loader_type>
    <sections config:type="list">
      <section>
        <append>__BOOTARGS__ resume=__ROOTPART__2 splash=silent crashkernel=128M-:64M@16M showopts </append>
        <initial>1</initial>
        <initrd>/boot/initrd-2.6.27.19-5-default</initrd>
        <lines_cache_id>0</lines_cache_id>
        <name>SUSE Linux Enterprise Server 11 - 2.6.27.19-5</name>
        <original_name>linux</original_name>
        <root>__ROOTPART__1</root>
        <type>image</type>
      </section>
      <section>
        <append>showopts ide=nodma apm=off noresume edd=off powersaved=off nohz=off highres=off processor.max_cstate=1 x11failsafe</append>
        <image>/boot/vmlinuz-2.6.27.19-5-default</image>
        <initrd>/boot/initrd-2.6.27.19-5-default</initrd>
        <lines_cache_id>1</lines_cache_id>
        <name>Failsafe -- SUSE Linux Enterprise Server 11 - 2.6.27.19-5</name>
        <original_name>failsafe</original_name>
        <root>__ROOTPART__1</root>
        <type>image</type>
      </section>
    </sections>
  </bootloader>
