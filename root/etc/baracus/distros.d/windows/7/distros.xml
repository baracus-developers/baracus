          <!-- the leading '/' in the file= indicates the path
               is relative to the 'builds' directory -->
          <sharefile sharetype='tftp' name='startrom.0'  file='/winstall/import/x86/startrom.0'/>
          <sharefile sharetype='tftp' name='BCD'         file='/winstall/import/x86/BCD'/>
          <sharefile sharetype='tftp' name='bootmgr.exe' file='/winstall/import/x86/bootmgr.exe'/>
          <sharefile sharetype='tftp' name='boot.sdi'    file='/winstall/import/x86/boot.sdi'/>
          <sharefile sharetype='tftp' name='boot.wim'    file='/winstall/import/x86/boot.wim'/>
        </sharefiles>
        <md5>c2a251ab154ec074289684432a67b343</md5>
        <url><![CDATA[]]></url>
      </iso>
    </product>
